{
  "name": "craftroots",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next lint && next build",
    "start": "next start -p 9211",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format:fix": "prettier --write \"**/*.+(js|jsx|json|css|md)\"",
    "prepare": "husky install",
    "lint-staged": "npx lint-staged"
  },
  "lint-staged": {
    "*.{html,js}": [
      "npx prettier --write"
    ],
    "*.js": [
      "npx eslint --fix"
    ]
  },
  "dependencies": {
    "@apollo/client": "^3.7.14",
    "autoprefixer": "10.4.14",
    "compressorjs": "^1.2.1",
    "dayjs": "^1.11.9",
    "eslint": "^8.39.0",
    "eslint-config-next": "13.3.4",
    "graphql": "^16.6.0",
    "lg-zoom": "^1.3.0",
    "next": "13.3.4",
    "nprogress": "^0.2.0",
    "postcss": "8.4.23",
    "prop-types": "^15.8.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.43.9",
    "react-intl": "^6.4.2",
    "react-redux": "^8.0.5",
    "react-scrollbar": "^0.5.6",
    "react-select": "^5.7.3",
    "react-share": "^4.4.1",
    "redux": "^4.2.1",
    "redux-thunk": "^2.4.2",
    "swiper": "^10.2.0",
    "tailwindcss": "3.3.2",
    "yet-another-react-lightbox": "^3.12.2"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^13.4.19",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^8.0.3"
  }
}
